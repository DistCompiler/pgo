{
	"name": "PGo",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"scalameta.metals",
				"github.vscode-github-actions",
				"scala-lang.scala",
				"golang.Go",
				"tlaplus.vscode-ide",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"postCreateCommand": "scala-cli clean . && scala-cli run . --main-class setup_sc"
}